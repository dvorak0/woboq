<doc f='include/boost/parameter/aux_/preprocessor/qualifier.hpp' l='51'>// Expands to the qualifier of x,
// where x is either a keyword qualifier or a keyword.
//
//   k =&gt; forward_reference
//   in(k) =&gt; in_reference
//   out(k) =&gt; out_reference
//   in_out(k) =&gt; in_out_reference
//   forward(k) =&gt; forward_reference
//   consume(k) =&gt; consume_reference
//   move_from(k) =&gt; move_from_reference</doc>
