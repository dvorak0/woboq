<mbr r='v4l2_selection::flags' o='64' t='__u32'/>
<mbr r='v4l2_selection::r' o='96' t='struct v4l2_rect'/>
<mbr r='v4l2_selection::reserved' o='224' t='__u32 [9]'/>
