<mbr r='_GdkVisual::byte_order' o='256' t='GdkByteOrder'/>
<mbr r='_GdkVisual::colormap_size' o='288' t='gint'/>
<mbr r='_GdkVisual::bits_per_rgb' o='320' t='gint'/>
<mbr r='_GdkVisual::red_mask' o='352' t='guint32'/>
<mbr r='_GdkVisual::red_shift' o='384' t='gint'/>
<mbr r='_GdkVisual::red_prec' o='416' t='gint'/>
<mbr r='_GdkVisual::green_mask' o='448' t='guint32'/>
