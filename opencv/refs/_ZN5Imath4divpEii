<doc f='include/OpenEXR/ImathFun.h' l='193'>//
// Integer division and remainder where the
// remainder of x/y is always positive:
//
//	divp(x,y) == floor (double(x) / double (y))
//	modp(x,y) == x - y * divp(x,y)
// </doc>
