<dec f='opencv/modules/gpu/include/opencv2/gpu/gpu.hpp' l='1058' type='void cv::gpu::histRange(const cv::gpu::GpuMat &amp; src, cv::gpu::GpuMat * hist, const cv::gpu::GpuMat * levels, cv::gpu::Stream &amp; stream = Stream::Null())'/>
<doc f='opencv/modules/gpu/include/opencv2/gpu/gpu.hpp' l='1053'>//! Calculates histogram with bins determined by levels array.
//! All levels must have one row and CV_32SC1 type if source has integer type or CV_32FC1 otherwise.
//! All channels of source are processed separately.
//! Supports CV_8UC4, CV_16UC4, CV_16SC4 and CV_32FC4 source types.
//! Output hist[i] will have one row and (levels[i].cols-1) cols and CV_32SC1 type.</doc>
<def f='opencv/modules/gpu/src/imgproc.cpp' l='993' ll='997' type='void cv::gpu::histRange(const cv::gpu::GpuMat &amp; src, cv::gpu::GpuMat * hist, const cv::gpu::GpuMat * levels, cv::gpu::Stream &amp; stream = Stream::Null())'/>
