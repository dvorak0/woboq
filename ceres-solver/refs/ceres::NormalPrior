<fun r='_ZNK5ceres11NormalPrior8EvaluateEPKPKdPdPS5_'/>
<mbr r='ceres::NormalPrior::A_' o='320' t='Matrix'/>
<mbr r='ceres::NormalPrior::b_' o='512' t='Vector'/>
