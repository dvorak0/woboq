<dec f='include/eigen3/Eigen/src/Core/MatrixBase.h' l='438' type='void Eigen::MatrixBase::applyHouseholderOnTheRight(const EssentialPart &amp; essential, const Scalar &amp; tau, Scalar * workspace)'/>
<def f='include/eigen3/Eigen/src/Householder/Householder.h' l='147' ll='167' type='void Eigen::MatrixBase::applyHouseholderOnTheRight(const EssentialPart &amp; essential, const Scalar &amp; tau, Scalar * workspace)'/>
<doc f='include/eigen3/Eigen/src/Householder/Householder.h' l='132'>/** Apply the elementary reflector H given by
  * \f$ H = I - tau v v^*\f$
  * with
  * \f$ v^T = [1 essential^T] \f$
  * from the right to a vector or matrix.
  *
  * On input:
  * \param essential the essential part of the vector \c v
  * \param tau the scaling factor of the Householder transformation
  * \param workspace a pointer to working space with at least
  *                  this-&gt;cols() * essential.size() entries
  *
  * \sa MatrixBase::makeHouseholder(), MatrixBase::makeHouseholderInPlace(), 
  *     MatrixBase::applyHouseholderOnTheLeft()
  */</doc>
